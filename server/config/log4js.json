{
  "appenders": {
    "out": {
      "type": "stdout"
    },
    "app": {
      "type": "dateFile",
      "filename": "./logs/blog",
      "pattern": "-yyyy-MM-dd.log",
      "alwaysIncludePattern": true
    }
  },
  "categories": {
    "default": {
      "appenders": [
        "out",
        "app"
      ],
      "level": "debug"
    }
  }
}